{
	"name": "async-agent",
	"version": "0.3.2",
	"description": "A javascript library of async functions",
	"main": "src/index.js",
	"scripts": {
		"test": "karma start --single-run",
		"test-watch": "karma start",
		"patch": "npm version patch -m \"Increment patch version to %s\" && npm publish",
		"minor": "npm version minor -m \"Increment minor version to %s\" && npm publish",
		"major": "npm version major -m \"Increment major version to %s\" && npm publish",
		"docs-readme": "jsdoc2md src/index.js > README.md -t docs/docs.hbs --plugin dmd-readable -d 3",
		"docs-defer": "jsdoc2md src/defer.js > docs/defer.md -t docs/docs.hbs --plugin dmd-readable -d 3",
		"docs-delay": "jsdoc2md src/delay.js > docs/delay.md -t docs/docs.hbs --plugin dmd-readable -d 3",
		"docs-clear": "jsdoc2md src/clear.js > docs/clear.md -t docs/docs.hbs --plugin dmd-readable -d 3",
		"docs-wait": "jsdoc2md src/wait.js > docs/wait.md -t docs/docs.hbs --plugin dmd-readable -d 3",
		"docs-forRange": "jsdoc2md src/forRange.js > docs/forRange.md -t docs/docs.hbs --plugin dmd-readable -d 3",
		"docs-debounce": "jsdoc2md src/debounce.js > docs/debounce.md -t docs/docs.hbs --plugin dmd-readable -d 3",
		"docs-throttle": "jsdoc2md src/throttle.js > docs/throttle.md -t docs/docs.hbs --plugin dmd-readable -d 3",
		"docs": "npm run docs-readme && npm run docs-defer && npm run docs-delay && npm run docs-clear && npm run docs-wait && npm run docs-forRange && npm run docs-debounce && npm run docs-throttle"
	},
