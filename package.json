{
	"name": "async-agent",
	"version": "0.4.4",
	"description": "A javascript library of async functions",
	"main": "index.js",
	"files": [
		"/src"
	],
	"sideEffects": false,
	"scripts": {
		"test": "karma start --single-run",
		"test-watch": "karma start",
		"version": "npm run docs && git add docs",
		"docs:readme": "jsdoc2md index.js > README.md",
		"docs:defer": "jsdoc2md src/defer.js > docs/defer.md",
		"docs:delay": "jsdoc2md src/delay.js > docs/delay.md",
		"docs:clear": "jsdoc2md src/clear.js > docs/clear.md",
		"docs:wait": "jsdoc2md src/wait.js > docs/wait.md",
		"docs:forRange": "jsdoc2md src/forRange.js > docs/forRange.md",
		"docs:debounce": "jsdoc2md src/debounce.js > docs/debounce.md",
		"docs:throttle": "jsdoc2md src/throttle.js > docs/throttle.md",
		"docs:waitBy": "jsdoc2md src/waitBy.js > docs/waitBy.md",
		"docs:rejectAfterBy": "jsdoc2md src/rejectAfterBy.js > docs/rejectAfterBy.md",
		"docs:rejectAfterWith": "jsdoc2md src/rejectAfterWith.js > docs/rejectAfterWith.md",
		"docs:rejectBy": "jsdoc2md src/rejectBy.js > docs/rejectBy.md",
		"docs:rejectWith": "jsdoc2md src/rejectWith.js > docs/rejectWith.md",
		"docs:resolveAfterBy": "jsdoc2md src/resolveAfterBy.js > docs/resolveAfterBy.md",
		"docs:resolveAfterWith": "jsdoc2md src/resolveAfterWith.js > docs/resolveAfterWith.md",
		"docs:resolveBy": "jsdoc2md src/resolveBy.js > docs/resolveBy.md",
		"docs:resolveWith": "jsdoc2md src/resolveWith.js > docs/resolveWith.md",
		"docs": "run-p \"docs:* -- -t docs/docs.hbs --plugin dmd-readable\""
	},
