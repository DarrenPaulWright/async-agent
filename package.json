{
	"name": "async-agent",
	"version": "0.3.0",
	"description": "A javascript library of async functions",
	"main": "src/index.js",
	"scripts": {
		"test": "karma start --single-run",
		"test-watch": "karma start",
		"patch": "npm version patch -m \"Increment patch version to %s\"",
		"minor": "npm version minor -m \"Increment minor version to %s\"",
		"major": "npm version major -m \"Increment major version to %s\"",
		"docs-readme": "jsdoc2md src/index.js > README.md --template docs.hbs --partial scope.hbs",
		"docs-defer": "jsdoc2md src/defer.js > docs/defer.md --template docs.hbs",
		"docs-delay": "jsdoc2md src/delay.js > docs/delay.md --template docs.hbs",
		"docs-clear": "jsdoc2md src/clear.js > docs/clear.md --template docs.hbs",
		"docs-wait": "jsdoc2md src/wait.js > docs/wait.md --template docs.hbs",
		"docs-forRange": "jsdoc2md src/forRange.js > docs/forRange.md --template docs.hbs",
		"docs-debounce": "jsdoc2md src/debounce.js > docs/debounce.md --template docs.hbs",
		"docs-throttle": "jsdoc2md src/throttle.js > docs/throttle.md --template docs.hbs",
		"docs": "npm run docs-readme && npm run docs-defer && npm run docs-delay && npm run docs-clear && npm run docs-wait && npm run docs-forRange && npm run docs-debounce && npm run docs-throttle"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/DarrenPaulWright/async-agent.git"
	},
	"keywords": [
		"defer",
		"delay",
		"clear",
		"wait",
		"promise",
		"forRange",
		"debounce",
		"throttle"
	],
	"author": "Darren Paul Wright <Darren@DarrenPaulWright.com>",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/DarrenPaulWright/async-agent/issues"
	},
	"homepage": "https://github.com/DarrenPaulWright/async-agent#readme",
	"devDependencies": {
		"@babel/core": "^7.4.5",
		"@babel/preset-env": "^7.4.5",
		"babel-eslint": "^10.0.1",
		"babel-loader": "^8.0.6",
		"babel-plugin-istanbul": "^5.1.4",
		"chai": "^4.2.0",
		"eslint": "^5.16.0",
		"eslint-loader": "^2.1.2",
		"eslint-plugin-babel": "^5.3.0",
		"jsdoc-to-markdown": "^5.0.0",
		"karma": "^4.1.0",
		"karma-brief-reporter": "0.1.1",
		"karma-chrome-launcher": "^2.2.0",
		"karma-coverage": "^1.1.2",
		"karma-coveralls": "^2.1.0",
		"karma-firefox-launcher": "^1.1.0",
		"karma-mocha": "^1.3.0",
		"karma-webpack": "^3.0.5",
		"mocha": "^6.1.4",
		"test-runner-config": "^0.5.0",
		"wallaby-webpack": "^3.9.15",
		"webpack": "^4.33.0",
		"webpack-cli": "^3.3.2"
	}
}
